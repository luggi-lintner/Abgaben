package Test;

import static org.junit.Assert.*;

import BST.BinaryTree;
import BST.BinaryTree.*;
import BST.Node.*;

public class Test {

	@org.junit.Test
	public void Test() {
	    
		BinaryTree bt = new BinaryTree();
		bt.addFromHead(10);
	    bt.addFromHead(5);
	    bt.addFromHead(15);
	    bt.addFromHead(12);
	    bt.addFromHead(20);
	    bt.addFromHead(1);
	    bt.addFromHead(4);
	    bt.deleteFromHead(1);
		
	 
	    assertTrue(bt.enthealtFromHead(10));
	    assertFalse(bt.enthealtFromHead(1));
	    assertTrue(bt.enthealtFromHead(12));
	    assertTrue(bt.enthealtFromHead(20));
	    
	    assertFalse(bt.enthealtFromHead(17));
	}

	

}
