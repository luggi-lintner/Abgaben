package test;

import static org.junit.Assert.*;

import org.junit.Test;

import LinkedList.linkedList;

public class test {

	@Test
	public void testAddValue() {
		linkedList ll = new linkedList();
		ll.add(5);
		assertEquals(5,5);
		
		
	}
	
	@Test
	public void testDelete()
	{
		linkedList ll = new linkedList();
		ll.add(5);
		ll.add(10);
		assertEquals(10,10);
		ll.add(15);
		ll.add(20);
		ll.delete(2);
		assertEquals(null,15);
		
	}

}
