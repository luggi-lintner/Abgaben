package Matrix;

public class Matrix {
	
	//int[][] matrix = new int [2][2];
	//int[][] matrix2 = new int [2][2];
		
	int matrix[][]={{1,2},
					{3,4}};
		
		
	int matrix2[][]={{1,1},
					{1,1}};
	
	
	public int abfrage(int a, int b){
		
		int erg = matrix[a][b];
		
		return erg;
		
	
	}
		

	


}
